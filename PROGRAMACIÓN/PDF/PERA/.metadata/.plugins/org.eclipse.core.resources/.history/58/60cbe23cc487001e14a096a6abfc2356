package repaso;

public class repaso2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		

		Integer[] listadenotas = new Integer[] {7,9,8,4,0,6,3,4,1,7,3,1,9,10};
		Integer media =0;
		Integer resultado=0;
		
		
		for(int i=0;i<listadenotas.length;i++){
			media= media + listadenotas[i];
			resultado= media/listadenotas.length-1;
			
		}
		System.out.println("\nMedia de notas: "+ resultado);
	}

}
